@*
    Public Pages handler: redirects index and content for the files generated by the loconomics-pages repo (Hugo static generator)    
*@
@{
    if (LcHelpers.IsVeryOldBrowser()) {
        @RenderPage(LcUrl.RenderAppPath + "en-US/HelpCenter/UpgradeBrowser.cshtml");
        return;
    }

    Server.Transfer("~/public/index.html");
}
